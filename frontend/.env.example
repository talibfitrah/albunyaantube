# Firebase Configuration
# Get these values from Firebase Console > Project Settings > Your Apps
VITE_FIREBASE_API_KEY=your-api-key-here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef

# Backend API
VITE_API_BASE_URL=http://localhost:8080

# YouTube Data API v3 (optional)
# When set, search calls YouTube directly instead of proxying through backend.
# Get a browser-restricted API key from Google Cloud Console.
# If not set, falls back to backend NewPipe proxy (no quota limits).
VITE_YOUTUBE_API_KEY=

# Firebase Emulator (optional for local development)
VITE_USE_FIREBASE_EMULATOR=false
