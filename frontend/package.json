{
  "name": "albunyaan-tube-admin",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "predev": "npm run generate:api",
    "build": "vue-tsc --noEmit && vite build",
    "prebuild": "npm run generate:api",
    "preview": "vite preview",
    "test": "timeout 300s vitest run",
    "pretest": "npm run generate:api",
    "test:e2e": "npm run build && playwright test",
    "generate:api": "openapi-typescript ../docs/architecture/api-specification.yaml -o src/generated/api/schema.ts"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "firebase": "^12.3.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-i18n": "^9.9.0",
    "vue-router": "^4.3.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.0",
    "@playwright/test": "^1.44.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/vue": "^7.0.0",
    "@types/node": "^20.11.19",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.4.5",
    "jsdom": "^22.1.0",
    "openapi-typescript": "^7.10.1",
    "terser": "^5.44.0",
    "typescript": "^5.4.2",
    "vite": "^5.2.0",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.12"
  }
}
