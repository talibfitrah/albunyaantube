{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "AuthLoginRequest",
  "type": "object",
  "required": ["email", "password"],
  "properties": {
    "email": { "type": "string", "format": "email", "maxLength": 190 },
    "password": { "type": "string", "minLength": 8, "maxLength": 128 }
  }
}
