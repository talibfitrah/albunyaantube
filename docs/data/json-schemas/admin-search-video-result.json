{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "AdminSearchVideoResult",
  "allOf": [
    { "$ref": "video-summary.json" },
    {
      "type": "object",
      "required": ["includeState", "parentChannelId"],
      "properties": {
        "includeState": {
          "type": "string",
          "enum": ["NOT_INCLUDED", "INCLUDED", "EXCLUDED"]
        },
        "parentChannelId": { "type": "string" },
        "parentPlaylistIds": {
          "type": "array",
          "items": { "type": "string" }
        }
      }
    }
  ]
}
