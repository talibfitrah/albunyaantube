{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "AdminSearchResponse",
  "type": "object",
  "required": ["query", "channels", "playlists", "videos"],
  "properties": {
    "query": { "type": "string" },
    "channels": {
      "type": "array",
      "items": { "$ref": "admin-search-channel-result.json" }
    },
    "playlists": {
      "type": "array",
      "items": { "$ref": "admin-search-playlist-result.json" }
    },
    "videos": {
      "type": "array",
      "items": { "$ref": "admin-search-video-result.json" }
    }
  },
  "additionalProperties": false
}
