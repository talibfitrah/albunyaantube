{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "AdminSearchPlaylistResult",
  "allOf": [
    { "$ref": "playlist-summary.json" },
    {
      "type": "object",
      "required": ["includeState", "parentChannelId", "excludedVideoCount"],
      "properties": {
        "includeState": {
          "type": "string",
          "enum": ["NOT_INCLUDED", "INCLUDED", "EXCLUDED"]
        },
        "parentChannelId": { "type": "string" },
        "excludedVideoCount": { "type": "integer", "minimum": 0 },
        "bulkEligible": { "type": "boolean", "default": true }
      }
    }
  ]
}
